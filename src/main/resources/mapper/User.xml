<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iteye.wwwcomy.dao.UserDao">
	<select id="loadUser" parameterType="java.lang.String"
		resultType="com.iteye.wwwcomy.model.User">
		select * from User U where U.name = #{userName};
	</select>
	<select id="getById" parameterType="java.lang.String"
		resultType="com.iteye.wwwcomy.model.User">
		select * from User U where U.userid = #{id};
	</select>
</mapper>